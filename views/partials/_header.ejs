<nav class="nav-bar">
  <div class="header">
    <img class="logo" src="/images/logo.png" />
    <div><i>Prefer.Engine</i></div>
  </div>
  <div class="logIn">
    <% if (username !== 'Guest') { %>
    <form class="logout-form" action="/home/logout" method="post">
      <div>Hello <%= username %></div>
      <input class="logoutbutton" name="logout" type="submit" value="Logout">
    </form>
    <% } else { %>
      <form class="logout-form">
      <div class="login-anchor"><a href="/">Log In?</a></div>
      </form>
    <% } %>
  </div>
</nav>
