<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Prefer.Engine</title>
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/main.css" type="text/css" />
    <link rel="stylesheet" href="/styles/_header.css" type="text/css" />
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=PT+Sans|PT+Serif" rel="stylesheet">


    <!--external vendor -->
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>

    <script type="text/javascript" src="/scripts/create-graph.js"></script>
    <script></script>



  </head>

  <body>

    <!--ejs partials -->
    <header>
      <% include partials/_header %>
    </header>

    <main id="container">
      <div class="poll-container">

      </div>


      <div id="history">
        <div id="new-poll">
          <a href="/new">Create a new poll?</a>
        </div>
        <% questions.forEach((questionName) => { %>
          <div class="panel panel-primary" data-question="<%= questionName[0] %>">
            <div class="panel-heading">
              <%= questionName[0] %>
            </div>
            <div class="panel-body">
              <li><a href="/result/<%= questionName[1] %>">See results</a></li>
              <li><a href="/polls/voter/<%= questionName[2] %>">Invite friends to vote</a></li>
            </div>
            <div class="panel-footer">
              <button class="delete" type="submit">Delete</button>
            </div>
          </div>
        </article>
        <% }) %>
      </div>
      <div class="enlarge-results"></div>
    </main>
  </body>
</html>
