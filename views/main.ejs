<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Decision Maker</title>
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/main.css" type="text/css" />
    <link rel="stylesheet" href="/styles/_header.css" type="text/css" />


    <!--external vendor -->
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>

    <script type="text/javascript" src="/scripts/create-graph.js"></script>
    <script></script>



  </head>

  <body>

    <!--ejs partials -->
    <header>
      <% include partials/_header %>
    </header>

    <main id="container">
    <div class="poll-container">

    </div>

    <div id="history">

      <% questions.forEach((ques) => { %>
        <div class="panel panel-primary" data-question="<%= ques.question %>">
          <div class="panel-heading">  <%= ques.question %> </div>
            <div class="panel-body">
              Results:
            </div>
          </div>
        <button class="delete" type="summit">delete</button>
      </article>
      <% }) %>
    </div>

    <div class="enlarge-results">


    </div>
  </main>
  </body>

</html>
